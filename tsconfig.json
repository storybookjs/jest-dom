{
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "target": "ES2020",
    "module": "CommonJS",
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictBindCallApply": true
  }
}
